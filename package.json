{
    "name": "capl-vector-ii",
    "displayName": "CAPL-II",
    "description": "Language Support for Vector CAPL Script in VS Code.",
    "version": "0.2.0",
    "publisher": "jlandseadel",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "capl",
            "aliases": ["Communication Access Programming Language (CAPL)", "capl", "CAPL"],
            "extensions": [".can",".cin"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "capl",
            "scopeName": "source.capl",
            "path": "./syntaxes/capl.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "capl",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jlandseadel/capl-vector.git"
    },
    "bugs": {
        "url": "https://github.com/jlandseadel/capl-vector/issues"
    }
}